{
  "ConnectionStrings": {
    "DefaultConnection": ""
  },
  "AzureAd": {
    "TenantId": "**tenant-id**",
    "ClientId": "**client-id**",
    "ClientSecret": "**client-secret**",
    "CallbackPath": "/signin-oidc",

    "BaseUrl": "https://**customHostname**",
    "Scopes": "openid email profile offline_access",
    "GraphResourceId": "https://graph.microsoft.com/",
    "GraphScopes": "User.Read User.Read.All Calendars.Read People.Read People.Read.All Contacts.Read Group.ReadWrite.All Notes.Create Notes.ReadWrite Notes.ReadWrite.All Mail.Send",
    "TeamTabResource": "**customHostname**/**client-id**"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
